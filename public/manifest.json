{
    "name": "Simple Color Picker",
    "description": "This is Simple Color Picker",
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": ["clipboardWrite"],
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "/images/icon.png",
            "32": "/images/icon.png",
            "48": "/images/icon.png",
            "128": "/images/icon.png"
        }
    },
    "icons": {
        "16": "/images/icon.png",
        "32": "/images/icon.png",
        "48": "/images/icon.png",
        "128": "/images/icon.png"
    }
}
